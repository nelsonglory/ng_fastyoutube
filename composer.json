{
	"name" : "phfr/ng_fastyoutube",
	"type" : "typo3-cms-extension",
	"description" : "TYPO3 Plugin that displays YouTube-videos faster by loading the video-player on demand while displaying a cacheable preview picture.",
	"homepage" : "https://github.com/nelsonglory/ng_fastyoutube",
	"license" : "GPL-2.0-or-later",
	"keywords" : [
		"TYPO3 CMS",
		"YouTube",
		"video",
		"responsive",
		"fast"
	],
	"support" : {
		"issues" : "https://github.com/nelsonglory/ng_fastyoutube"
	},
	"require" : {
		"typo3/cms-core" : "^12.4"
	},
	"authors" : [{
			"name" : "Roderick Braun",
			"role" : "Developer",
			"email" : "roderick.braun@ph-freiburg.de",
			"homepage" : "https://www.ph-freiburg.de"
		}
	],
	"autoload" : {
		"psr-4" : {
			"PHFR\\NgFastyoutube\\" : "Classes/"
		}
	},
	"replace" : {
		"typo3-ter/ng_fastyoutube" : "self.version"
	},
	"extra" : {
		"typo3/cms" : {
			"extension-key" : "ng_fastyoutube"
		}
	},
	"minimum-stability" : "stable"
}
